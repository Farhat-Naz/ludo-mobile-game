{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@engine/*": ["src/engine/*"],
      "@ui/*": ["src/ui/*"],
      "@navigation/*": ["src/navigation/*"],
      "@state/*": ["src/state/*"],
      "@services/*": ["src/services/*"],
      "@utils/*": ["src/utils/*"],
      "@types/*": ["src/types/*"]
    }
  }
}
